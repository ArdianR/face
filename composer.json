{
    "name": "eduardostuart/face",
    "keywords": ["face detection"],
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/http": "^5.4",
        "illuminate/support": "^5.4"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0|~5.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Eduardo Stuart",
            "email": "hi@s.tuart.me"
        }
    ],
    "autoload": {
        "psr-4": {
            "Face\\": "src/face"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
